<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="false">
   <fileset name="Java source for test code" enabled="true" check-config-name="JTS Checks for Tests" local="false">
      <file-match-pattern match-pattern="^src/test/.*.java" include-pattern="true" />
   </fileset>
   <fileset name="Java source for production code" enabled="true" check-config-name="JTS Checks" local="false">
      <file-match-pattern match-pattern="^src/.*.java" include-pattern="true" />
      <file-match-pattern match-pattern="^src/test/.*.java" include-pattern="false" />
   </fileset>
   <filter name="NonSrcDirs" enabled="true" />
</fileset-config>